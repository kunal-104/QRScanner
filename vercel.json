{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "GOOGLE_CREDENTIALS": "@google_credentials",
      "SPREADSHEET_ID": "@spreadsheet_id",
      "GOOGLE_API_KEY": "@google_api_key"
    }
  }